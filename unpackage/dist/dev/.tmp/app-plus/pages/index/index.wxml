<view class="uni-tab-bar"><scroll-view class="uni-swiper-tab" id="tab-bar" scroll-x scroll-left="{{scrollLeft}}"><block wx:for="{{tabBars}}" wx:for-item="tab" wx:for-index="index" wx:key="id"><view class="{{['swiper-tab-list',tabIndex==index?'active':'']}}" id="{{tab.id}}" data-current="{{index}}" data-event-opts="{{[['tap',[['tapTab',[index]]]]]}}" bindtap="__e"><view class="tabitems">{{''+tab.name+''}}</view></view></block></scroll-view><swiper class="swiper-box" current="{{tabIndex}}" duration="{{300}}" data-event-opts="{{[['change',[['changeTab',['$event']]]]]}}" bindchange="__e"><block wx:for="{{newsitems}}" wx:for-item="tab" wx:for-index="index1" wx:key="index1"><swiper-item><scroll-view class="list" scroll-y data-event-opts="{{[['scrolltolower',[['loadMore',[index1]]]]]}}" bindscrolltolower="__e"><block wx:if="{{tabBars[index1].template==='index'}}">{{''+tab.index+''}}<block wx:for="{{tab.data}}" wx:for-item="v" wx:for-index="i" wx:key="i"><block><block wx:if="{{v.type==='swiper'}}"><swiper-image vue-id="{{'1-'+index1+'-'+i}}" resdata="{{v.data}}" bind:__l="__l"></swiper-image></block><block wx:else><block wx:if="{{v.type==='indexnavs'}}"><index-nav vue-id="{{'2-'+index1+'-'+i}}" resdata="{{v.data}}" bind:__l="__l"></index-nav><divider vue-id="{{'3-'+index1+'-'+i}}" bind:__l="__l"></divider></block><block wx:else><block wx:if="{{v.type==='threeAdv'}}"><thress-adv vue-id="{{'4-'+index1+'-'+i}}" resdata="{{v.data}}" bind:__l="__l"></thress-adv><divider vue-id="{{'5-'+index1+'-'+i}}" bind:__l="__l"></divider></block><block wx:else><block wx:if="{{v.type==='oneAdv'}}"><card vue-id="{{'6-'+index1+'-'+i}}" headTitle="{{v.data.title}}" bodyCover="{{v.data.cover}}" bind:__l="__l"></card></block><block wx:else><block wx:if="{{v.type==='list'}}"><view class="row j-sb"><block wx:for="{{v.data}}" wx:for-item="vitem" wx:for-index="vindex" wx:key="vindex"><block><common-list vue-id="{{'7-'+index1+'-'+i+'-'+vindex}}" item="{{vitem}}" index="{{vindex}}" bind:__l="__l"></common-list></block></block></view></block></block></block></block></block></block></block><view class="uni-tab-bar-loading">{{''+tab.loadingText+''}}</view></block><block wx:if="{{tabBars[index1].template==='special'}}"><block wx:for="{{tab.data}}" wx:for-item="v" wx:for-index="i" wx:key="i"><block><block wx:if="{{v.type==='swiper'}}"><swiper-image vue-id="{{'8-'+index1+'-'+i}}" resdata="{{v.data}}" bind:__l="__l"></swiper-image></block><block wx:else><block wx:if="{{v.type==='indexnavs'}}"><index-nav vue-id="{{'9-'+index1+'-'+i}}" resdata="{{v.data}}" bind:__l="__l"></index-nav><divider vue-id="{{'10-'+index1+'-'+i}}" bind:__l="__l"></divider></block><block wx:else><block wx:if="{{v.type==='list'}}"><card vue-id="{{'11-'+index1+'-'+i}}" headTitle="热卖爆破" bind:__l="__l" vue-slots="{{['default']}}"><view class="row j-sb"><block wx:for="{{v.data}}" wx:for-item="vitem" wx:for-index="vindex" wx:key="vindex"><block><common-list vue-id="{{('12-'+index1+'-'+i+'-'+vindex)+','+('11-'+index1+'-'+i)}}" item="{{vitem}}" index="{{vindex}}" bind:__l="__l"></common-list></block></block></view></card></block></block></block></block></block></block></scroll-view></swiper-item></block></swiper></view>